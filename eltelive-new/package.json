{
  "name": "eltelive",
  "version": "1.0.0",
  "description": "Very simple, battle tested live streaming solution of ELTE University",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "node server/server.js",
    "watch": "nodemon server/server.js",
    "test:api": "mocha server/test/test_api/test.js --exit",
    "test:utils": "mocha server/test/test_utils --exit",
    "test": "mocha server/test --exit"
  },
  "dependencies": {
