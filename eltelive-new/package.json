    "vue-chartjs": "^3.5.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off"
    }
  },
  "resolutions": {
    "eslint-loader": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gerazo/eltelive"
  },
  "keywords": [
    "node",
    "vue",
    "express",
    "mongodb"
  ],
  "license": "GPL"
