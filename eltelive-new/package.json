  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "~4.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^5.0.0",
    "ps-list": "^7.2.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
