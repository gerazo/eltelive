    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chart.js": "^3.5.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.6.4",
    "eslint-plugin-babel": "^5.3.1",
    "express": "^4.17.1",
    "flv.js": "^1.5.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mocha": "^8.3.2",
    "mongoose": "^5.11.17",
    "network-speed": "^2.1.1",
    "node-fetch": "^3.0.0",
    "node-media-server": "^2.3.8",
    "node-os-utils": "^1.3.5",
    "nodemon": "^2.0.7",
    "npm": "^7.9.0",
    "popper.js": "^1.16.1",
    "shortid": "^2.2.16",
    "stylus-loader": "^4.1.1",
    "video.min.js": "^1.2.3-0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.6",
    "vuex": "^4.0.0-beta.4",
    "ws": "^8.2.3"
